scoreColorScale = d3.scale.quantile()
                    .domain([0, 0.5, 0.6, 1])
                    .range(['#DA4F49', '#F89406', '#51A351'])

scoreClassScale = d3.scale.quantile()
                    .domain([0, 0.5, 0.6, 1])
                    .range(['c', 'b', 'a'])


# Bounding box for Israel
